<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="es.rafa.cmstraining.myBatisMapper.Mapper">

    <select id="getAllUsers" resultType="es.rafa.cmstraining.DAO.UserDAO">
        SELECT
        id as id,
        first_name as name,
        last_name as lastName
        FROM users
    </select>

</mapper>